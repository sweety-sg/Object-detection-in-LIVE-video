    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "img = cv2.imread(\"images/cycle.jpg\")\n",
    "\n",
    "# flipping an image\n",
    "img_flip = cv2.flip(img, -1)\n",
    "\n",
    "cv2.imshow(\"window\", img_flip)\n",
    "cv2.imwrite('images/cycle_f.jpg', img_flip)\n",
    "cv2.waitKey(0)"
